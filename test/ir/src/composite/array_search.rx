fn find_max(arr: [i32; 4]) -> i32 {
    let mut max: i32 = arr[0];
    let mut i: usize = 1;
    while (i < 4) {
        if (arr[i] > max) {
            max = arr[i];
        }
        i = i + 1;
    }
    max
}

fn main() {
    let nums: [i32; 4] = [3, 9, 2, 7];
    printlnInt(find_max(nums));
    exit(0);
}
